            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_2"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_1" />

        <Button
            android:id="@+id/g1l3_button_12"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_2"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_3"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_1"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_2" />

        <Button
            android:id="@+id/g1l3_button_13"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_2"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_3"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_2"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_3" />

        <Button
            android:id="@+id/g1l3_button_14"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_2"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_3"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_3"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_4" />

        <Button
            android:id="@+id/g1l3_button_15"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_2"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_3"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_4"
            app:layout_constraintRight_toRightOf="parent" />





        <Button
            android:id="@+id/g1l3_button_16"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_3"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_4"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_1" />

        <Button
            android:id="@+id/g1l3_button_17"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_3"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_4"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_1"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_2" />

        <Button
            android:id="@+id/g1l3_button_18"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_3"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_4"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_2"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_3" />

        <Button
            android:id="@+id/g1l3_button_19"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_3"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_4"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_3"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_4" />

        <Button
            android:id="@+id/g1l3_button_20"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_3"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_4"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_4"
            app:layout_constraintRight_toRightOf="parent" />





        <Button
            android:id="@+id/g1l3_button_21"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_1" />

        <Button
            android:id="@+id/g1l3_button_22"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_1"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_2" />

        <Button
            android:id="@+id/g1l3_button_23"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_2"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_3" />

        <Button
            android:id="@+id/g1l3_button_24"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_3"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_4" />

        <Button
            android:id="@+id/g1l3_button_25"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_4"
            app:layout_constraintRight_toRightOf="parent" />






