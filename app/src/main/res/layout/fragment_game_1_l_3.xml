<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/colorSecondary"
    tools:context=".ui.tasks.game1.Game1l3Fragment">

    <ImageView
        android:id="@+id/g1_logo"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/gl_g1_h_1"
        app:layout_constraintBottom_toTopOf="@+id/gl_g1_h_2"
        android:src="@drawable/ic_memory"
        app:layout_constraintLeft_toRightOf="parent" />

    <TextView
        android:id="@+id/g1l3_top_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:textSize="16sp"
        tools:text="Remaining:1234567890"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gl_g1_h_2"
        app:layout_constraintBottom_toTopOf="@+id/gl_g1_h_3"
        android:textAlignment="center"
        android:textColor="?attr/colorPrimaryVariant"
        android:visibility="visible" />

    <FrameLayout
        android:id="@+id/g1l3_start_layout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/gl_g1_h_3"
        app:layout_constraintBottom_toTopOf="@+id/gl_g1_h_4"
        android:visibility="invisible">

        <ImageButton
            android:id="@+id/g1l3_start_button"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintLeft_toRightOf="parent"
            android:background="?attr/colorButtonWaiting"
            android:src="@drawable/ic_baseline_touch"/>



    </FrameLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/g1l3_text_layout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/gl_g1_h_3"
        app:layout_constraintBottom_toTopOf="@+id/gl_g1_h_4"
        android:visibility="invisible">

        <TextView
            android:id="@+id/g1l3_middle_text"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:gravity="center"
            android:textColor="?attr/colorPrimary"
            android:textStyle="bold"
            app:autoSizeMaxTextSize="100sp"
            app:autoSizeMinTextSize="12sp"
            app:autoSizeTextType="uniform"
            tools:text="123456789" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/g1l3_grid_layout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/gl_g1_h_3"
        app:layout_constraintBottom_toTopOf="@+id/gl_g1_h_4"
        android:visibility="invisible">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/gl_g1_grid_h_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.2" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/gl_g1_grid_h_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.4" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/gl_g1_grid_h_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.6" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/gl_g1_grid_h_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.8" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/gl_g1_grid_v_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.20" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/gl_g1_grid_v_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.40" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/gl_g1_grid_v_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.60" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/gl_g1_grid_v_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.80" />



        <Button
            android:id="@+id/g1l3_button_1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/gl_g1_grid_v_1"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/g1l3_button_2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintLeft_toRightOf="@+id/gl_g1_grid_v_1"
            app:layout_constraintRight_toRightOf="@id/gl_g1_grid_v_2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_1"/>

        <Button
            android:id="@+id/g1l3_button_3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintLeft_toRightOf="@+id/gl_g1_grid_v_2"
            app:layout_constraintRight_toRightOf="@id/gl_g1_grid_v_3"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_1"/>

        <Button
            android:id="@+id/g1l3_button_4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintLeft_toRightOf="@+id/gl_g1_grid_v_3"
            app:layout_constraintRight_toRightOf="@id/gl_g1_grid_v_4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_1"/>

        <Button
            android:id="@+id/g1l3_button_5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintLeft_toRightOf="@+id/gl_g1_grid_v_4"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_1"/>



        <Button
            android:id="@+id/g1l3_button_6"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_1"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_1" />

        <Button
            android:id="@+id/g1l3_button_7"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_1"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_2"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_1"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_2" />

        <Button
            android:id="@+id/g1l3_button_8"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_1"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_2"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_2"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_3" />

        <Button
            android:id="@+id/g1l3_button_9"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_1"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_2"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_3"
            app:layout_constraintRight_toLeftOf="@id/gl_g1_grid_v_4" />

        <Button
            android:id="@+id/g1l3_button_10"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="@id/gl_g1_grid_h_1"
            app:layout_constraintBottom_toTopOf="@id/gl_g1_grid_h_2"
            app:layout_constraintLeft_toLeftOf="@id/gl_g1_grid_v_4"
            app:layout_constraintRight_toRightOf="parent" />




        <Button
